{
  "name": "pluto-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --base-href=/pluto-ui/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy-pack-install-start": "npm run deploy && npm run pack && npm run installpkg && ng serve",
    "deploy-pack-install": "npm run deploy && npm run pack && npm run installpkg",
    "deploy": "cd ./projects/ngx-pluto/ && ng-packagr -p ng-package.json && scss-bundle -c scss-bundle.config.json && scss-bundle -c scss-style.config.json && scp -r ../../projects/ngx-pluto/src/theme ../../dist/ngx-pluto/",
    "pack": "cd ./dist/ngx-pluto/ && npm pack",
    "installpkg": "npm install ./dist/ngx-pluto/ngx-pluto-0.0.37.tgz",
    "publish": "cd ./dist/ngx-pluto/ && npm publish",
    "doc": "compodoc --config projects/ngx-pluto/.compodocrc.json --language zh-CN"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.16",
    "@angular/cdk": "~7.3.0",
    "@angular/common": "^7.2.16",
    "@angular/compiler": "^7.2.16",
    "@angular/core": "^7.2.16",
    "@angular/elements": "^7.2.16",
    "@angular/forms": "^7.2.16",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/router": "^7.2.16",
    "core-js": "^2.5.4",
    "cropperjs": "^1.5.7",
    "jquery": "^3.3.1",
    "jquery.fancytree": "^2.30.2",
    "moment": "^2.27.0",
    "rxjs": "~6.6.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.0",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "^7.2.16",
    "@angular/language-service": "^7.2.16",
    "@compodoc/compodoc": "^1.1.11",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.3.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.1.0",
    "protractor": "~5.4.0",
    "scp": "0.0.3",
    "scss-bundle": "^2.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.4"
  }
}
